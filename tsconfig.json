{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@layouts": ["./@layouts"],
      "@layouts/*": ["./@layouts/*"],
      "@pages": ["./pages"],
      "@pages/*": ["./pages/*"],
      "@types": ["./@types"],
      "@types/*": ["./@types/*"],
      "@styles": ["./@styles"],
      "@styles/*": ["./@styles/*"],
      "@hooks": ["./@hooks"],
      "@hooks/*": ["./@hooks/*"],
      "@components": ["./@components"],
      "@components/*": ["./@components/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
